sample1 = `
export default class Foo extends React.Component {
  render() {
    const { foo, bar } = this.props;
    return (
      <div>
        This is {foo} {bar}
      </div>
    )
  }
}
`;


const sample2 = `
const Foo = ({ foo }) => {
  return (
    <div>
      Foo {foo}
    </div>
  )
}

export default Foo
`

const sample3 = `
export default ({ foo }) => {
  return (
    <div>
      Foo
    </div>
  )
}
`
